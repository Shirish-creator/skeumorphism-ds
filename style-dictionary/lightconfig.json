{
  "source": ["./style-dictionary/token-studio/Light-transformed-tokens.json"],
  "platforms": {
    "css": {
      "transformGroup": "css",
      "buildPath": "./src/style-dictionary-dist/",
      "files": [
        {
          "destination": "light-variables.css",
          "format": "css/variables",
          "options": {
